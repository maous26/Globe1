# GlobeGenius Docker Environment Variables
# Copy this file to .env and fill in your actual values

# JWT Configuration
JWT_SECRET=your_jwt_secret_here_min_32_chars_long

# SendGrid Email Service
SENDGRID_API_KEY=your_sendgrid_api_key
SENDGRID_FROM_EMAIL=alertes@globegenius.app
SENDGRID_FROM_NAME=GlobeGenius Alerts

# Flight APIs
FLIGHT_API_URL=https://goflightlabs.com/advanced-flights-schedules
FLIGHT_API_KEY=your_flightlabs_api_key

# AI Services
GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/gemini-1.5-flash-latest:generateContent
GOOGLE_AI_API_KEY=your_google_ai_api_key
GPT_API_ENDPOINT=https://api.openai.com/v1/chat/completions
OPENAI_API_KEY=your_openai_api_key

# Database Configuration
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=secure_mongo_password_123
REDIS_PASSWORD=secure_redis_password_123

# Mongo Express (optional admin interface)
MONGOEXPRESS_LOGIN=admin
MONGOEXPRESS_PASSWORD=admin_password_123

# Application Configuration
NODE_ENV=production 